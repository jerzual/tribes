{
  "version": 1,
  "projects": {
    "tribes-api": {
      "root": "apps/tribes-api",
      "sourceRoot": "apps/tribes-api/src",
      "projectType": "application",
      "prefix": "tribes-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/tribes-api",
            "main": "apps/tribes-api/src/main.ts",
            "tsConfig": "apps/tribes-api/tsconfig.app.json",
            "assets": ["apps/tribes-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/tribes-api/src/environments/environment.ts",
                  "with": "apps/tribes-api/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "tribes-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tribes-api/**/*.ts",
              "apps/tribes-api/**/*.spec.ts",
              "apps/tribes-api/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/tribes-api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/tribes-api"]
        }
      },
      "tags": [],
      "implicitDependencies": ["tribes-model", "tribes-engine"]
    },
    "tribes-engine": {
      "root": "libs/tribes-engine",
      "sourceRoot": "libs/tribes-engine/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tribes-engine/**/*.js",
              "libs/tribes-engine/**/*.jsx",
              "libs/tribes-engine/**/*.ts",
              "libs/tribes-engine/**/*.tsx",
              "libs/tribes-engine/**/*.spec.ts",
              "libs/tribes-engine/**/*.spec.tsx",
              "libs/tribes-engine/**/*.spec.js",
              "libs/tribes-engine/**/*.spec.jsx",
              "libs/tribes-engine/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/tribes-engine/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/tribes-engine"]
        }
      },
      "tags": []
    },
    "tribes-model": {
      "root": "libs/tribes-model",
      "sourceRoot": "libs/tribes-model/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tribes-model/**/*.ts",
              "libs/tribes-model/**/*.spec.ts",
              "libs/tribes-model/**/*.spec.tsx",
              "libs/tribes-model/**/*.spec.js",
              "libs/tribes-model/**/*.spec.jsx",
              "libs/tribes-model/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/tribes-model/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/tribes-model"]
        }
      },
      "tags": []
    },
    "tribes-web": {
      "root": "apps/tribes-web",
      "sourceRoot": "apps/tribes-web/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/tribes-web",
            "index": "apps/tribes-web/src/index.html",
            "main": "apps/tribes-web/src/main.tsx",
            "polyfills": "apps/tribes-web/src/polyfills.ts",
            "tsConfig": "apps/tribes-web/tsconfig.app.json",
            "assets": [
              "apps/tribes-web/src/favicon.ico",
              "apps/tribes-web/src/assets"
            ],
            "styles": ["apps/tribes-web/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/tribes-web/src/environments/environment.ts",
                  "with": "apps/tribes-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "livereload": {
              "fileReplacements": [
                {
                  "replace": "apps/tribes-web/src/environments/environment.ts",
                  "with": "apps/tribes-web/src/environments/environment.hmr.ts"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "tribes-web:build",
            "proxyConfig": "apps/tribes-web/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "tribes-web:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tribes-web/**/*.js",
              "apps/tribes-web/**/*.jsx",
              "apps/tribes-web/**/*.ts",
              "apps/tribes-web/**/*.tsx",
              "apps/tribes-web/**/*.spec.ts",
              "apps/tribes-web/**/*.spec.tsx",
              "apps/tribes-web/**/*.spec.js",
              "apps/tribes-web/**/*.spec.jsx",
              "apps/tribes-web/**/*.d.ts"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/tribes-web/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/tribes-web"]
        }
      },
      "tags": [],
      "implicitDependencies": ["tribes-model", "tribes-engine"]
    },
    "tribes-web-e2e": {
      "root": "apps/tribes-web-e2e",
      "sourceRoot": "apps/tribes-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tribes-web-e2e/cypress.json",
            "tsConfig": "apps/tribes-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "tribes-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tribes-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tribes-web-e2e/src/**/*.ts",
              "apps/tribes-web-e2e/src/**/*.js"
            ]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": [],
      "implicitDependencies": ["tribes-web"]
    }
  }
}
